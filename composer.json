{
    "name": "namelivia/travelperk-http-php",
    "description": "PHP SDK for accessing the TravelPerk API",
    "license": "MIT",
    "keywords": ["travelperk", "sdk", "api"],
    "authors": [
        {
            "name": "Jos√© Ignacio Amelivia Santiago",
            "email": "jignacio.amelivia@gmail.com",
            "homepage": "https://namelivia.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "nesbot/carbon": "^2.0",
        "kamermans/guzzle-oauth2-subscriber": "^1.0",
        "namelivia/travelperk-php-api-types": "^1.2.9",
        "json-mapper/json-mapper": "^2.6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Namelivia\\TravelPerk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Namelivia\\TravelPerk\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
